@import url("https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/css/bootstrap.min.css");
@import url("https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.7.7/css/mdb.min.css");

.stars-container {
    position: relative;
    display: inline-block;
    color: transparent;
  }
  
  .stars-container:before{
    position: absolute;
    top: 0;
    left: 0;
    content: '★★★★★';
    color:lightgray;
  }
  .stars-over {
    position: absolute;
    top: 0;
    left: 0;
    overflow: hidden;
  }

  .container{
    -ms-flex-wrap: nowrap | wrap | wrap-reverse;
    flex-wrap: nowrap | wrap | wrap-reverse;
  }